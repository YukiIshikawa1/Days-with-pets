<div class="container">
  <h1>Posts#index</h1>
    <p>Find me in app/views/posts/index.html.erb</p>
  <div class=row>
    <div class="col-6 mx-auto">
      <% @post.each do |post| %>
        <div class="image">
          <%= attachment_image_tag post, :image, size:"200x200" %>
        </div>
        <% end %>
    </div>    
  </div>
</div>

  
